{
    "family": "my-api-task",
    "containerDefinitions": [
        {
            "name": "<CONTAINER_NAME>",
            "image": "<IMAGE>",
            "portMappings": [
                {
                    "containerPort": 3000
                }
            ],
            "essential": true
	}
    ],
    "executionRoleArn": "<TASK_ROLE>",
    "networkMode": "awsvpc",
    "volumes": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072"
}
